<Application xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ServerEntry.Dashboard"
    xmlns:themes="clr-namespace:Material.Styles.Themes;assembly=Material.Styles"
    xmlns:icon="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
    x:Class="ServerEntry.Dashboard.App">
    <!-- RequestedThemeVariant="Default" -->
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <themes:MaterialTheme PrimaryColor="Indigo" SecondaryColor="Indigo" />
        <icon:MaterialIconStyles />
        <!-- <FluentTheme /> -->
    </Application.Styles>
</Application>